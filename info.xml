<?xml version="1.0"?>
<extension key="de.systopia.custommailheader" type="module">
  <file>custommailheader</file>
  <name>Custom Mail Header</name>
  <description>Small extension that adds a custom Field to Mails send by CiviCRM. The Header can be configured by a small configuration form.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Philipp Batroff</author>
    <email>batroff@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/pbatroff/customMailHeader</url>
    <url desc="Documentation">https://github.com/pbatroff/customMailHeader</url>
    <url desc="Support">https://github.com/pbatroff/customMailHeader/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2018-02-09</releaseDate>
  <version>0.2.alpha</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>4.7</ver>
  </compatibility>
  <comments>Small and Simple Extension to add a Custom Mail Header in CiviCRM</comments>
  <civix>
    <namespace>CRM/Custommailheader</namespace>
  </civix>
</extension>
